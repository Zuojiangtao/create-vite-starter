{
  "name": "vite-template-starter",
  "version": "1.0.0",
  "description": "A vite-based front-end startup template generator.",
  "author": "zuojt <zuojt2022@gmail.com> (https://github.com/Zuojiangtao)",
  "license": "ISC",
  "keywords": [
    "vue",
    "react",
    "nuxt",
    "next",
    "cli",
    "template",
    "starter"
  ],
  "main": "./dist/index.js",
  "files": [
    "dist/index.js",
    "template",
    "locales"
  ],
  "bin": {
    "create-vite-starter": "./dist/index.js",
    "vite-start": "./dist/index.js"
  },
  "engines": {
    "node": "^14.18.0 || ^16.0.0 || >=18.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "dev": "pnpm run build --watch",
    "build": "tsup",
    "clean:lib": "rimraf node_modules",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,tsx,jsx,vue}": [
      "prettier --write",
      "eslint --cache --fix",
      "eslint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.3.0",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "chalk": "^5.3.0",
    "ejs": "^3.1.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "rimraf": "~5.0.10",
    "tsup": "~8.2.4",
    "typescript": "~5.5.4"
  }
}
